algoritmo "Partida_Estrela_Triangulo"

var
   comando : inteiro
   ciclo : inteiro
   m: caractere

inicio

  
  escreval("Deseja iniciar o motor? (S/N)")
  leia(m)

  se m = "S" entao
  escreval("Motor Ligado em Estrela")
  para ciclo de 1 ate 5 faca
  escreval ("Ciclo", ciclo)
  fimpara
  senao se m = "N" entao
  escreval ("Motor não iniciado")
  fimse
  fimse

  escreval("Motor Ligado em triângulo")
  


fimalgoritmo
