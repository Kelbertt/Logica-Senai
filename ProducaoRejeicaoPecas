algoritmo "Controle de Produção e Rejeição de Peças"

var
   Peso: inteiro
   TPeças: inteiro
   AP: inteiro
   RP: inteiro

inicio

   AP <- 0
   RP <- 0

   escreval("Quantas Peças Serao Lidas?" )
   leia(TPeças)
   se (Peso >= 490) entao e (Peso <= 510) entao
      escreval("Peça foi aprovada")
      AP <- AP + 1
   senao 
      escreval("Peça não foi aprovada")
      RP <- RP + 1

fimalgoritmo
